#include <iostream>

using namespace std;

int main()
{
    int a;
    int b;
    char z;
    cout<<"enter a e b"<<endl;
    cout<<"where e is +,-,*,/,%"<<endl;
    cin>>a>>z>>b;
    switch(z)
    {
        case '+' : cout<<a+b;break;
        case '-' : cout<<a-b;break;
        case '*' : cout<<a*b;break;
        case '/' : cout<<a/b;break;
       default : cout<<"did not understood";
    }
        return 0;
}
